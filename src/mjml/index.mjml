    <mj-section padding="0" background-color="#241e10">
      <mj-column padding="60px 8.333333%" padding-top="20px" width="100%">
        <mj-button width="70%">Register your interest</mj-button>
      </mj-column>
    </mj-section>
    <mj-section padding="0 50px" background-color="#241e10">
      <mj-column>
        <mj-image padding="20px 0" width="91px" src="images/logoProjet.png" alt=""/>
        <mj-text padding="0 75px" padding-bottom="10px" align="center" font-family="'Montserrat', Arial, Helvetica, sans-serif" mj-class="T36 cTW0 fw0" line-height="40px" text-transform="uppercase">A new altitude in working life</mj-text>
        <mj-text padding="0 66.5px" align="center" mj-class="T16 cTW0 fw0" line-height="22px">31 Above is a new sculptural landmark in Dubai Maritime City. Every element, from its stacked geometry to its refined public interiors, has been crafted to enhance performance, wellbeing, and connectivity.</mj-text>
      </mj-column>
    </mj-section>
    <mj-section padding="25px 50px" background-color="#241e10">
      <mj-column>
        <mj-button width="100%">Register your interest</mj-button>
      </mj-column>
    </mj-section>
    <mj-section padding="0" padding-top="15px" background-color="#241e10">
      <mj-column>
        <mj-image padding="0" width="578px" src="images/building.png" alt=""/>
      </mj-column>
    </mj-section>
    <mj-section padding="0" background-color="#241e10">
      <mj-group>
        <mj-column padding-top="40px" padding-left="50px" width="63%">
          <mj-text padding="0" mj-class="T16 cTW0 fw0" line-height="22px">31 Above is a sculptural commercial landmark in Dubai Maritime City, designed to redefine modern working life through architecture rooted in balance, precision, and wellness. Rising in stacked volumes with landscaped terraces, the tower brings natural light, greenery, and perspective into every level, creating a workspace that inspires focus and connection.</mj-text>
        </mj-column>
        <mj-column width="37%">
          <mj-image padding="0" align="right" width="220px" src="images/suiteBuilding.png" alt=""/>
        </mj-column>
      </mj-group>
    </mj-section>
    <mj-section padding="25px 50px" background-color="#241e10">
      <mj-column>
        <mj-button width="100%">Register your interest</mj-button>
      </mj-column>
    </mj-section>
    <mj-section background-color="#241e10">
      <mj-column vertical-align="middle" width="46%">
        <mj-image padding="0" align="left" width="274px" src="images/vueCiel.png" alt=""/>
      </mj-column>
      <mj-column padding-left="20px" vertical-align="middle" width="54%">
        <mj-spacer height="30px"></mj-spacer>
        <mj-text padding="0" padding-bottom="10px" mj-class="T24 fw2 cTW0">Location</mj-text>
        <mj-text padding="0" padding-bottom="10px" mj-class="T16 fw0 cTW0"><span style="color:#9A7A56;font-weight:700">20 min |</span> <span style="text-transform:capitalize">Dubai international airport</span></mj-text>
        <mj-text padding="0" padding-bottom="10px" mj-class="T16 fw0 cTW0"><span style="color:#9A7A56;font-weight:700">15 min |</span> <span style="text-transform:capitalize">Dubai mall</span></mj-text>
        <mj-text padding="0" padding-bottom="10px" mj-class="T16 fw0 cTW0"><span style="color:#9A7A56;font-weight:700">10 min |</span> Nearest school</mj-text>
        <mj-text padding="0" padding-bottom="10px" mj-class="T16 fw0 cTW0"><span style="color:#9A7A56;font-weight:700;">10 min |</span> <span style="text-transform:capitalize">Jumeirah beach</span> coastal line</mj-text>
        <mj-text padding="0" padding-bottom="10px" mj-class="T16 fw0 cTW0"><span style="color:#9A7A56;font-weight:700">05 min |</span> <span style="text-transform:capitalize">Mina rashid</span></mj-text>
        <mj-text padding="0" padding-bottom="10px" mj-class="T16 fw0 cTW0"><span style="color:#9A7A56;font-weight:700;">10 min |</span> Nearest hospital</mj-text>
        <mj-text padding="0" padding-bottom="10px" mj-class="T16 fw0 cTW0"><span style="color:#9A7A56;font-weight:700">15 min |</span> <span style="text-transform:capitalize">The gold souk</span></mj-text>
        <mj-spacer height="30px"></mj-spacer>
